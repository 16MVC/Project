        $(document).ready(function () {
            //我的车票
            $("#link_4_myTicket").on("click", function () {
                //window.location.href = "/MyTicket/queryOrder";
            });
            //未完成订单
            $("#link_4_NonComOrder").on("click", function () {

                $("#link_4_myTicket").parent().siblings().removeClass("prev");
                $("#link_4_myTicket").parent().addClass("prev");
                $("#dd_4_NonComOrder").addClass("cur");
                $("#dd_4_NonComOrder").siblings().removeClass("cur");
                $(".nav-label").css("top", "58.881px");
                $(".nav-label").css("display", "block");
                $("#ifMain").attr("src", "/MyTicket/initNoComplete");

            });
            //已完成订单
            $("#link_4_comOrder").on("click", function () {
                $("#dd_4_comOrder").siblings().removeClass("prev");
                $("#dd_4_NonComOrder").addClass("prev");
                $("#dd_4_comOrder").addClass("cur");
                $("#dd_4_comOrder").siblings().removeClass("cur");
                $(".nav-label").css("top", "98.881px");
                $(".nav-label").css("display", "block");
                $("#ifMain").attr("src", "/MyTicket/queryOrder");
            });
            //我的保险
            $("#link_4_ticketInfo").on("click", function () {
                $("#dd_4_comOrder").siblings().removeClass("prev");
                $("#dd_4_comOrder").addClass("prev");
                $("#link_4_ticketInfo").parent().addClass("cur");
                $("#link_4_ticketInfo").parent().siblings().removeClass("cur");
                $(".nav-label").css("top", "138.702px");
                $(".nav-label").css("display", "block");
                $("#ifMain").attr("src", "/MyTicket/insurance");
            });
            //个人信息
            $("#link_4_myInfor").on("click", function () {
                //window.location.href = "/MyTicket/insurance";
            });
            //查看个人信息
            $("#link_4_showMyInfor").on("click", function () {
                $("#link_4_myInfor").parent().siblings().removeClass("prev");
                $("#link_4_myInfor").parent().addClass("prev");
                $("#dd_4_showMyInfor").addClass("cur");
                $("#dd_4_showMyInfor").siblings().removeClass("cur");
                $(".nav-label").css("top", "218.702px");
                $(".nav-label").css("display", "block");
                $("#ifMain").attr("src", "/MyTicket/initQueryUserInfo");
            });
            //账号安全
            $("#link_4_showSafe").on("click", function () {
                $("#dd_4_showMyInfor").siblings().removeClass("prev");
                $("#dd_4_showMyInfor").addClass("prev");
                $("#dd_4_showSafe").addClass("cur");
                $("#dd_4_showSafe").siblings().removeClass("cur");
                $(".nav-label").css("top", "258.643px");
                $(".nav-label").css("display", "block");
                $("#ifMain").attr("src", "/MyTicket/userSecurity");
            });
            //手机核验
            $("#link_4_sjhy").on("click", function () {
                $("#dd_4_showSafe").siblings().removeClass("prev");
                $("#dd_4_showSafe").addClass("prev");
                $("#dd_4_sjhy").addClass("cur");
                $("#dd_4_sjhy").siblings().removeClass("cur");
                $(".nav-label").css("top", "298.583px");
                $(".nav-label").css("display", "block");
                $("#ifMain").attr("src", "/MyTicket/bindTel");
            });
            //常用信息管理
            $("#link_4_passengerInfor").on("click", function () {
                window.location.href = "/MyTicket/passengers";
            });
            //常用联系人
            $("#link_4_passengerIns").on("click", function () {
                window.location.href = "/MyTicket/passengers";
            });
            //贴心服务
            $("#dd_4_qxyy").on("click", function () {
                window.location.href = "/MyTicket/qxyyInfo";
            });
            //重点旅客预约
            $("#dd_4_qxyy").on("click", function () {
                window.location.href = "/MyTicket/qxyyInfo";
            });
            //遗失物品查找
            $("#dd_4_lostitems").on("click", function () {
                window.location.href = "/MyTicket/lostInfo";
            });
        });
    </script>

