                    $("#fromStationText").css("color", "#333");
                } else {
                    $("#toStationText").val("" + $('.ac_even openLi ac_odd').val() + "");
                    $("#toStationText").css("color", "#333");
                }
                $("#form_cities2").css("display", "none");
            });
            //选择首字母
            $.stationForTicket = {
                js: function (N) {
                    var M;
                    for (M = 1; M <= 7; M++) {
                        if (M == N) {
                            $("#ul_list" + M).css("display", "block");
                            $("#nav_list" + M).addClass("action");
                        }
                        else {
                            $("#ul_list" + M).css("display", "none");
                            $("#nav_list" + M).removeClass("action")
                        }
                    }
                },
                closeShowCity: function () {
                    $("#form_cities2").css("display", "none");
                    i = 0;
                },
                pageDesigh: function () {
                    alert("下一页");
                },
            }


        });
        $.extend({
