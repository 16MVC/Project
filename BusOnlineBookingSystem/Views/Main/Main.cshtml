    <script type="text/javascript">
        //设置时间格式
        function myformatter(date) {
            var y = date.getFullYear();
            var m = date.getMonth() + 1;
            var d = date.getDate();
            return y + '-' + (m < 10 ? ('0' + m) : m) + '-' + (d < 10 ? ('0' + d) : d);
        }
        //检索日期格式
        function myparser(s) {
            if (!s) return new Date();
            var ss = (s.split('-'));
            var y = parseInt(ss[0], 10);
            var m = parseInt(ss[1], 10);
            var d = parseInt(ss[2], 10);
            if (!isNaN(y) && !isNaN(m) && !isNaN(d)) {
                return new Date(y, m - 1, d);
            } else {
                return new Date();
            }
        }
        //限制选择小于当天的日期
        $(function () {
            $('#train_date').datebox().datebox('calendar').calendar({
                validator: function (date) {
                    var now = new Date();
                    var d1 = new Date(now.getFullYear(), now.getMonth(), now.getDate());
                    var d2 = new Date(now.getFullYear(), now.getMonth(), now.getDate() + 60);
                    return d1 <= date && date <= d2;
                }
            })

        });
        $(document).ready(function () {
            //通告栏鼠标移入移出
            $(".slideTxt .hd li").mouseover(function () {
                $(this).addClass("on");
                $(this).siblings().removeClass("on");
            });
            $("#fromStationText").on("focus",
            function () {
                if ($(this).val() == "简拼/全拼/汉字") {
                    $(this).val("");
                    $(this).css("color", "#333")
                }
            }).on("blur",
            function () {
                if ($(this).val() == "") {
                    $(this).val("简拼/全拼/汉字");
                    $(this).css("color", "#999")
                }
            });

            $("#toStationText").on("focus",
            function () {
                if ($(this).val() == "简拼/全拼/汉字") {
                    $(this).val("");
                    $(this).css("color", "#333")
                }
            }).on("blur",
            function () {
                if ($(this).val() == "") {
                    $(this).val("简拼/全拼/汉字");
                    $(this).css("color", "#999")
                }
            });
            //查询
            $("#a_search_ticket").on("click",
            function () {
                //if ($("#fromStationText").val().trim() == "" || $("#fromStationText").val() == "简拼/全拼/汉字" ||
                //    $("#toStationText").val().trim() == ""   || $("#toStationText").val() == "简拼/全拼/汉字") {
                //    alert("请填写出发地与目的地");
                //}else {

                //}

                $.ajax({
                    url: "/Main/getBusticket", 
                    type: "get",
                    dataType: "json",
                    success: function (result) {
                        window.location.href = "/Buyticket/TicketBook";
                    }, error: function () {

                    }
                })
            })
            //往返
            $("#wf").on("click",
             function () {
                 $("#_wf_span_date").removeclass("color999");
                 //$("#back_train_date").
             })
            单程
            $("#dc").on("click",
             function () {
                 $("#_wf_span_date").addClass("color999");
             })
            //点击选择出发地
            var wave;
            var i = 0;
            $("#from_station_imageB").click(function () {
                if (i == 0) {
                    $("#form_cities2").css("display", "block");
                    $("#form_cities2").css("top", "30.0952px");
                    i = 1; wave = true;
                } else {
                    $("#form_cities2").css("display", "none");
                    i = 0;
                }

            });
            //点击选择目的地
            $("#to_station_imageB").click(function () {
                if (i == 0) {
                    $("#form_cities2").css("display", "block");
                    $("#form_cities2").css("top", "65px");
                    i = 1; wave = false;
                } else {
                    $("#form_cities2").css("display", "none");
                    i = 0;
                }

